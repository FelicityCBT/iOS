source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'
use_frameworks!

target 'Feelicity' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  pod 'Alamofire'
  pod 'AlamofireNetworkActivityIndicator', '~> 2.2'
  pod 'Firebase'
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'GoogleSignIn'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'GoogleAnalytics'
  
  # Pods for Feelicity

  target 'FeelicityTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FeelicityUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
